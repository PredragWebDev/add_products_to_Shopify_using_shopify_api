                //insertAtCaret(source, (ch == "Enter" ? (window.opera ? '\r\n' : '\n') : ch));

                if (shift == 1) {
                    shift = 0;
                    shiftpress(shift);
                }
            }
            break;
        case '&lt;':
            if (val.length < 15) {
                source.value += '<';
                //ch = '>';
                //insertAtCaret(source, (ch == "Enter" ? (window.opera ? '\r\n' : '\n') : ch));

                if (shift == 1) {
                    shift = 0;
                    shiftpress(shift);
                }
            }
            break;
        case 'Shift':
            //                        if (shift == 0) {
            //                            shift = 1;
            //                        }
            //                        else {
            //                            shift = 0;
            //                        }
            //                        shiftpress(shift);
            //                        break;
        case 'Caps':

            break;
        default:
            if (val.length < 15) {
                source.value += ch;
                //insertAtCaret(source, (ch == "Enter" ? (window.opera ? '\r\n' : '\n') : ch));

                if (shift == 1) {
                    shift = 0;
                    shiftpress(shift);
                }
            }
            break;
    }
}
}

function shiftpress(shift) {
    shiftkey('0', shift);
    shiftkey('1', shift);
    shiftkey('2', shift);
    shiftkey('3', shift);
}

function capspress(caps) {

    capskey('0', caps);
    capskey('1', caps);
    capskey('2', caps);
    capskey('3', caps);
}

function shiftkey(linenum, shift) {
    var noofkey = 0;
    var source;
    switch (linenum) {
        case '0':
            noofkey = 14;
            if (shift == 1) {
                source = line0s;
            }
            else {
                source = line0;
            }
            break;
        case '1':
            noofkey = 15;
            if (shift == 1) {
                source = line1s;
            }
            else {
                source = line1;
            }
            break;
        case '2':
            noofkey = 14;
            if (shift == 1) {
                source = line2s;
            }
            else {
                source = line2;
            }
            break;
        case '3':
            noofkey = 12;
            if (shift == 1) {
                source = line3s;
            }
            else {
                source = line3;
            }
            break;
    }
    for (i = 0; i <= source.length - 1; i++) {
        var td = document.getElementById('key' + linenum.toString() + i.toString());
        var span = td.childNodes[0];
        var ch = span.innerHTML;
        span.innerHTML = source[i];
    }
}

function capskey(linenum, caps) {
    var noofkey = 0;
    switch (linenum) {
        case '0':
            noofkey = 14;
            break;
        case '1':
            noofkey = 15;
            break;
        case '2':
            noofkey = 14;
            break;
        case '3':
            noofkey = 12;
            break;
    }
    for (i = 0; i <= noofkey - 1; i++) {
        var td = document.getElementById('key' + linenum.toString() + i.toString());
        var span = td.childNodes[0];
        var ch = span.innerHTML;
        if (filter.test(ch)) {
            switch (ch) {
                case 'Backspace':
                    break;
                case 'Tab':
                    break;
                case 'Caps':
                    break;
                case 'Shift':
                    break;
                case 'Enter':
                    break;
                default:
                    if (caps == 1) {
                        ch = ch.toUpperCase();
                        span.innerHTML = ch;
                    }
                    else {
                        ch = ch.toLowerCase();
                        span.innerHTML = ch;
                    }
                    break;
            }
        }

    }
}
