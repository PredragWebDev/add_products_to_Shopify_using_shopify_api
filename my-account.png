    <script type="text/javascript">
        window.onload = function () {
            checkb();
        };
        window.onresize = function () {
            checkb();
        };
        function isMacintosh() {
            return navigator.platform.indexOf('Mac') > -1
        }
        function checkb() {
            if (isMobile.iphone() || isMobile.ipad()) {
                return false;
            }
            var isSafari = navigator.vendor && navigator.vendor.indexOf('Apple') > -1;
            if ((navigator.userAgent.indexOf("MSIE") != -1) || (!!document.documentMode == true)) //IF IE > 10
            {
                var deviceXDPI = screen.deviceXDPI;
                if (deviceXDPI > 96) {
                    getScale('1.2');
                } else if (deviceXDPI < 96) {
                    getScale('0.8');
                }
                else {
                    getScale('1');
                }
            }
            else if (isSafari == true) {
                screenOrientedWidth = screen.width;
                if (window.orientation == 90) {
                    screenOrientedWidth = screen.height;
                }
                var char = screenOrientedWidth / window.innerWidth;
                getScale(char);
            }
            else {
                getScale(window.devicePixelRatio);
            }
        }
        function getScale(scale) {
            var isSafari = navigator.vendor && navigator.vendor.indexOf('Apple') > -1;
            if (isMobile.ipad() || isMobile.iphone()) {
                if (scale > 1) {
                    zoomAlert("<img src='Images/login/notification_warning.png' style='width:6vh;height:6vh;float:left;margin:0vh 2vh 0vh 0vh;' /><div style='width:82%;float:right'><p style='line-height:1.3vh;margin-top:2.5vh;'>Your Browser seems to be <strong>zoomed in</strong>.</p></div>", 5000);
                    document.getElementById("zoompop").style.width = '50vh';
                }
                else if (scale < 1) {
                    zoomAlert("<img src='Images/login/notification_warning.png' style='width:6vh;height:6vh;float:left;margin:0vh 2vh 0vh 0vh;' /><div style='width:82%;float:right'><p style='line-height:1.3vh;margin-top:2.5vh;'>Your Browser seems to be <strong>zoomed out</strong>.</p></div>", 5000);
                    document.getElementById("zoompop").style.width = '50vh';
                }
                else {
                    document.getElementById('zoompop').style.display = 'none';
                }
            }
            else if (isMacintosh() == true) {
                if (scale > 1) {
                    zoomAlert("<img src='Images/login/notification_warning.png' style='width:6vh;height:6vh;float:left;margin:0vh 0vh 0vh 0vh;' /><div style='width:88%;float:right'><p style='line-height:1.3vh;margin-top:1vh;'>Your Browser seems to be <strong>zoomed in</strong>.</p><p style='line-height:1.3vh'> Press Command & 0 at the same time to reset it.</p></div>", 5000);
                }
                else if (scale < 1) {
                    zoomAlert("<img src='Images/login/notification_warning.png' style='width:6vh;height:6vh;float:left;margin:0vh 0vh 0vh 0vh;' /><div style='width:88%;float:right'><p style='line-height:1.3vh;margin-top:1vh;'>Your Browser seems to be <strong>zoomed out</strong>.</p><p style='line-height:1.3vh'> Press Command & 0 at the same time to reset it.</p></div>", 5000);
                }
                else {
                    document.getElementById('zoompop').style.display = 'none';
                }
            }
            else {
                if (scale > 1) {
                    zoomAlert("<img src='Images/login/notification_warning.png' style='width:6vh;height:6vh;float:left;margin:0vh 0vh 0vh 0vh;' /><div style='width:88%;float:right'><p style='line-height:1.3vh;margin-top:1vh;'>Your Browser seems to be <strong>zoomed in</strong>.</p><p style='line-height:1.3vh'> Press CTRL & 0 at the same time to reset it.</p></div>", 5000);
                }
                else if (scale < 1) {
                    zoomAlert("<img src='Images/login/notification_warning.png' style='width:6vh;height:6vh;float:left;margin:0vh 0vh 0vh 0vh;' /><div style='width:88%;float:right'><p style='line-height:1.3vh;margin-top:1vh;'>Your Browser seems to be <strong>zoomed out</strong>.</p><p style='line-height:1.3vh'> Press CTRL & 0 at the same time to reset it.</p></div>", 5000);
                }
                else {
                    document.getElementById('zoompop').style.display = 'none';
                }
            }
        }
        function zoomAlert(msg, duration) {

            document.getElementById("zoompop").style.display = 'inline';
            var el = document.getElementById("divmsg");
            el.innerHTML = msg;
        }
